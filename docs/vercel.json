{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "astro",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/docs/speech-to-text/pre-recorded-audio/:path*",
      "destination": "/docs/pre-recorded-audio/:path*",
      "permanent": true
    },
    {
      "source": "/docs/speech-to-text/universal-streaming/:path*",
      "destination": "/docs/universal-streaming/:path*",
      "permanent": true
    },
    {
      "source": "/docs/lemur/:path*",
      "destination": "/docs/llm-gateway/:path*",
      "permanent": true
    },
    {
      "source": "/docs/audio-intelligence/:path*",
      "destination": "/docs/speech-understanding/:path*",
      "permanent": true
    },
    {
      "source": "/docs/speech-to-text/pre-recorded-audio",
      "destination": "/docs/pre-recorded-audio",
      "permanent": true
    },
    {
      "source": "/docs/speech-to-text/universal-streaming",
      "destination": "/docs/universal-streaming",
      "permanent": true
    },
    {
      "source": "/docs/pre-recorded-audio/supported-languages",
      "destination": "/docs/pre-recorded-audio/language-detection#supported-languages",
      "permanent": true
    },
    {
      "source": "/docs/guides/content-safety-detection",
      "destination": "/docs/content-moderation",
      "permanent": true
    },
    {
      "source": "/docs/overview/real-time-transcription",
      "destination": "/docs/universal-streaming",
      "permanent": true
    },
    {
      "source": "/docs/speech-to-text/streaming",
      "destination": "/docs/universal-streaming",
      "permanent": true
    },
    {
      "source": "/docs/api-reference/lemur/:path*",
      "destination": "/docs/api-reference",
      "permanent": true
    },
    {
      "source": "/docs/api-reference/upload",
      "destination": "/docs/api-reference",
      "permanent": true
    },
    {
      "source": "/docs/sentiment-analysis",
      "destination": "/docs/speech-understanding/sentiment-analysis",
      "permanent": true
    },
    {
      "source": "/docs/entity-detection",
      "destination": "/docs/speech-understanding/entity-detection",
      "permanent": true
    },
    {
      "source": "/docs/topic-detection",
      "destination": "/docs/speech-understanding/topic-detection",
      "permanent": true
    },
    {
      "source": "/docs/auto-chapters",
      "destination": "/docs/speech-understanding/auto-chapters",
      "permanent": true
    },
    {
      "source": "/docs/key-phrases",
      "destination": "/docs/speech-understanding/key-phrases",
      "permanent": true
    },
    {
      "source": "/docs/security-compliance",
      "destination": "https://www.assemblyai.com/security",
      "permanent": true
    },
    {
      "source": "/docs/concepts/:path*",
      "destination": "/docs",
      "permanent": true
    },
    {
      "source": "/docs/getting-started",
      "destination": "/docs/getting-started/transcribe-an-audio-file",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/docs/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/docs/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
